{
  "name": "politica-cookies-br",
  "version": "1.0.2",
  "description": "Interface para gerenciar consentimento de cookies em aplicações web.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.global.js",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "./style.css": "./dist/style.css",
  "./dist/style.css": "./dist/style.css",
  "scripts": {
    "build": "tsup src/index.ts && tsx src/variables-scss.ts && sass src/styles/styles.scss dist/style.css --style=compressed",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "dotenv": "17.2.2",
    "sass": "1.92.0",
    "tsx": "4.20.5",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  }
}
